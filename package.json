{
  "name": "backend2",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:auth": "cross-env NODE_ENV=test jest src/tests/auth.fixed.test.js",
    "test:auth-comprehensive": "cross-env NODE_ENV=test jest src/tests/auth.comprehensive.test.js",
    "test:phase2": "cross-env NODE_ENV=test jest --testPathPattern='(trips|ai|search|phase2)' --runInBand",
    "test:trips": "cross-env NODE_ENV=test jest trips.management.test.js --runInBand",
    "test:ai": "cross-env NODE_ENV=test jest ai.integration.test.js --runInBand",
    "test:search": "cross-env NODE_ENV=test jest search.functionality.test.js --runInBand",
    "test:integration": "cross-env NODE_ENV=test jest phase2.integration.test.js --runInBand"
  },
  "keywords": [
    "backend",
    "api",
    "authentication",
    "travel"
  ],
  "author": "",
  "license": "ISC",
  "description": "TravelSense v2 Backend API",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.3",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.6.2",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup/jest.phase2.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/tests/**",
      "!src/config/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "forceExit": true,
    "detectOpenHandles": true,
    "testTimeout": 15000
  }
}
